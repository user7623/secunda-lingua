<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.diplomska">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SET_ALARM" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Diplomska">
        <service android:name=".AlarmService"></service>
        <activity android:name=".TranslateActivity"></activity>
        <activity android:name=".TestOneActivity"></activity>
        <activity android:name=".TestActivity"></activity>
        <activity android:name=".AchievementsActivity"></activity>
        <activity android:name=".DownloadMoreAction"></activity>
        <activity android:name=".SettingsActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VocabularyActivity" />
        <activity android:name=".UnderstandActivity" />
        <receiver android:name=".BroadcastReceiver"></receiver>
        <receiver android:name=".AlertReceiver" />
    </application>

</manifest>